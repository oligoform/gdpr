{
	"name": "georgringer/gdpr",
	"type": "typo3-cms-extension",
	"description": "Make TYPO3 more compatible to GDPR",
	"keywords": [
		"TYPO3",
		"extension",
		"gdpr",
		"dsgvo"
	],
	"authors": [
		{
			"name": "Georg Ringer",
			"role": "Developer"
		}
	],
	"license": [
		"GPL-2.0-or-later"
	],
	"require": {
		"typo3/cms-core": "^8.7 || ^9",
		"fzaninotto/faker": "^1.7"
	},
	"autoload": {
		"psr-4": {
			"GeorgRinger\\Gdpr\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"GeorgRinger\\Gdpr\\Tests\\": "Tests"
		}
	},
	"replace": {
		"gdpr": "self.version",
		"typo3-ter/gdpr": "self.version"
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	},
	"scripts": {
		"post-autoload-dump": [
			"mkdir -p .Build/Web/typo3conf/ext/",
			"[ -L .Build/Web/typo3conf/ext/gdpr ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/gdpr"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		},
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web"
		}
	}
}
